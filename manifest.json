{
    "manifest_version": 3,
    "name": "Chat314 extension",
    "version": "1.0",
    "description": "Adds a chatbot to any webpage.",
    "permissions": ["activeTab", "scripting", "storage"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.js"]
    }
  ]
    
  }
  
